Execute (Test Lock):
  let my_lock = langserver#lock#semaphore()

  AssertEqual v:false, my_lock.locked

  call my_lock.lock()
  AssertEqual v:true, my_lock.locked

  call my_lock.unlock()
  AssertEqual v:false, my_lock.locked

  AssertEqual -1, my_lock.get_id()

  call my_lock.set_id(1)
  AssertEqual 1, my_lock.get_id()
